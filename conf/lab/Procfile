todo_service: java -Xms512m -Xmx512m -javaagent:cp-trace-agent.jar -Dspring.profiles.active=todo-service -Dcatchpoint.property.profile=todo-service-lab -jar todo-app.jar
user_service: java -Xms512m -Xmx512m -javaagent:cp-trace-agent.jar -Dspring.profiles.active=user-service -Dcatchpoint.property.profile=user-service-lab -jar todo-app.jar