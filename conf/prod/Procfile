todo_service: java -Xms512m -Xmx512m -javaagent:cp-trace-agent.jar -Dspring.profiles.active=todo-service  -Dcatchpoint.property.profile=todo-service-prod -jar todo-app.jar
user_service: java -Xms512m -Xmx512m -javaagent:cp-trace-agent.jar -Dspring.profiles.active=user-service  -Dcatchpoint.property.profile=user-service-prod -jar todo-app.jar