todo_service: java -Xms256m -Xmx256m -javaagent:thundra-agent-bootstrap.jar -Dspring.profiles.active=todo-service  -Dthundra.agent.property.profile=todo-service-prod -jar todo-app.jar
user_service: java -Xms256m -Xmx256m -javaagent:thundra-agent-bootstrap.jar -Dspring.profiles.active=user-service  -Dthundra.agent.property.profile=user-service-prod -jar todo-app.jar