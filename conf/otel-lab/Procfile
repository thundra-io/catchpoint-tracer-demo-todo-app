todo_service: java -Xms512m -Xmx512m -javaagent:otel-agent.jar -Dspring.profiles.active=todo-service -Dotel.javaagent.configuration-file=config-todo-service-otel-lab.properties -jar todo-app.jar
user_service: java -Xms512m -Xmx512m -javaagent:otel-agent.jar -Dspring.profiles.active=user-service -Dotel.javaagent.configuration-file=config-user-service-otel-lab.properties -jar todo-app.jar