todo_service: java -Xms512m -Xmx512m -javaagent:otel-agent.jar -Dspring.profiles.active=todo-service -Dotel.javaagent.configuration-file=config-todo-service-otel-staging.properties -Dotel.instrumentation.aws-sdk.experimental-use-propagator-for-messaging=true -jar todo-app.jar
user_service: java -Xms512m -Xmx512m -javaagent:otel-agent.jar -Dspring.profiles.active=user-service -Dotel.javaagent.configuration-file=config-user-service-otel-staging.properties -jar todo-app.jar